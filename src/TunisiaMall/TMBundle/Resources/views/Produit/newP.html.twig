
{% extends "TunisiaMallTMBundle::resEnsView/EIndex.html.twig" %}


{% block content %}
 <div class="side-body padding-top">
     <h3  class="text-success">Ajouter Produit </h3>
<form action="{{ path('produit_create') }}" method="post" {{ form_enctype(form) }}
      class="form-horizontal" role="form" enctype="multipart/form-data">
    {{ form_errors(form) }}

   
    
    <div class="form-group">
        <label for="description" class="control-label col-sm-3 " style="text-align: left;">
            {{ form_label(form.nomenseigne ,"Nom Enseigne") }}
        </label>
        <div class="col-sm-9">
            {{ form_widget(form.nomenseigne) }}
        </div>
        <div class="col-sm-12">
            {{ form_errors(form.nomenseigne) }}
        </div>

        </div>  
        
        <div class="form-group">
        <label for="description" class="control-label col-sm-3 " style="text-align: left;">
            {{ form_label(form.idboutique ,"Id Boutique") }}
        </label>
        <div class="col-sm-9">
            {{ form_widget(form.idboutique) }}
        </div>
        <div class="col-sm-12">
            {{ form_errors(form.idboutique) }}
        </div>

    </div>  
        
        
        <div class="form-group">
        <label for="description" class="control-label col-sm-3 " style="text-align: left;">
            {{ form_label(form.nom ,"Produit") }}
        </label>
        <div class="col-sm-9">
            {{ form_widget(form.nom) }}
        </div>
        <div class="col-sm-12">
            {{ form_errors(form.nom) }}
        </div>
        </div>
          <div class="form-group">
        <label for="description" class="control-label col-sm-3 " style="text-align: left;">
            {{ form_label(form.ptsfidelite,"Points") }}
        </label>
        <div class="col-sm-9">
            {{ form_widget(form.ptsfidelite) }}
        </div>
        <div class="col-sm-12">
            {{ form_errors(form.ptsfidelite) }}
        </div>
       </div>
           <div class="form-group">
        <label for="description" class="control-label col-sm-3 " style="text-align: left;">
            {{ form_label(form.info,"Informations") }}
        </label>
        <div class="col-sm-9">
            {{ form_widget(form.info) }}
        </div>
        <div class="col-sm-12">
            {{ form_errors(form.info) }}
        </div>
       </div>
           <div class="form-group">
        <label for="description" class="control-label col-sm-3 " style="text-align: left;">
            {{ form_label(form.file,"Produit") }}
        </label>
        <div class="col-sm-9">
            {{ form_widget(form.file) }}
        </div>
        <div class="col-sm-12">
            {{ form_errors(form.file) }}
        </div>
       </div>
         {{ form_rest(form) }}
        </form>
    
 </div>
 </div>
   {% endblock %}

{% block footer %} 
    
    
  {{ parent() }}
   

{% endblock %}